:halt    is op_00_07=0x05 { halt(); }
:ei imm8 is op_00_07=0x06 ; imm8 {
    local tmp:1 = imm8;
    enable_interrupt(tmp);
}
:reti    is op_00_07=0x07 {
    pop16(SR);
    pop32(PC);
    return [PC];
}
:ld (addr8),imm8   is op_00_07=0x08 ; addr8 ; imm8 { addr8 = imm8; }
:push imm8         is op_00_07=0x09 ; imm8 { local tmp:1 = imm8; push8(tmp); }
:ldw (addr8),imm16 is op_00_07=0x0a ; addr8 ; imm16 { addr8 = imm16; }
:pushw imm16       is op_00_07=0x0b ; imm16 { local tmp:2 = imm16; push16(tmp); }
:incf              is op_00_07=0x0c {
    $(RFP) = $(RFP) + 1;
    if ($(MAX) == 0) goto <end>;
    $(RFP) = $(RFP) & 0b011;
<end>
}
:decf              is op_00_07=0x0d {
    $(RFP) = $(RFP) - 1;
    if ($(MAX) == 0) goto <end>;
    $(RFP) = $(RFP) & 0b011;
<end>
}
:ret               is op_00_07=0x0e {
    pop32(PC);
    return [PC];
}
:retd imm16        is op_00_07=0x0f ; imm16 {
    pop32(PC);
    XSP = XSP + imm16;
    return [PC];
}

:rcf         is op_00_07=0x10 { $(H_flag) = 0; $(V_flag) = 0; $(C_flag) = 0; }
:scf         is op_00_07=0x11 { $(H_flag) = 0; $(V_flag) = 0; $(C_flag) = 1; }
:ccf         is op_00_07=0x12 { $(N_flag) = 0; $(C_flag) = ~$(C_flag); }
:zcf         is op_00_07=0x13 { $(N_flag) = 0; $(C_flag) = ~$(Z_flag); }
:push A      is op_00_07=0x14 & A { push16(A); }
:pop A       is op_00_07=0x15 & A { pop16(A); }
:ex F,^"F'"  is op_00_07=0x16 & F { F = F ^ F; }
:ldf imm8    is op_00_07=0x17 ; imm8 {
    $(RFP) = imm8 & 0b111;
    if ($(MAX) == 0) goto <end>;
    $(RFP) = $(RFP) & 0b011;
<end>
}
:push F      is op_00_07=0x18 & F { push16(F); }
:pop F       is op_00_07=0x19 & F { pop16(F); }
